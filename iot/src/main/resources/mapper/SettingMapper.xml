<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iot.mapper.SettingMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.iot.entity.Setting">
        <id column="id" property="id" />
        <result column="low_temp" property="lowTemp" />
        <result column="high_temp" property="highTemp" />
        <result column="low_hum" property="lowHum" />
        <result column="high_hum" property="highHum" />
        <result column="user_id" property="userId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, low_temp, high_temp, low_hum, high_hum, user_id
    </sql>

</mapper>
